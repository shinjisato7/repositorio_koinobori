{
  "name": "ember-lifeline",
  "version": "3.1.1",
  "description": "Ember.js addon for lifecycle aware async tasks and DOM events.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rwjblue/ember-lifeline.git"
  },
  "scripts": {
    "build": "ember build",
    "docs": "ember ts:precompile && documentation build --document-exported \"addon/index.js\" --config documentation.yml -f md -o API.md && prettier --single-quote --trailing-comma es5 --write API.md && ember ts:clean",
    "start": "ember server",
    "test": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "lint-staged": {
    "package.json,{app,addon,config,tests}/**/*.{js,json}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.3"
  },
  "devDependencies": {
    "@types/ember": "^3.0.1",
    "@types/ember-qunit": "^3.0.2",
    "@types/ember-test-helpers": "^1.0.0",
    "@types/ember-testing-helpers": "^0.0.3",
    "@types/qunit": "^2.5.1",
    "@types/rsvp": "^4.0.1",
    "broccoli-asset-rev": "^3.0.0",
    "documentation": "^9.1.1",
    "ember-ajax": "^4.0.0",
    "ember-cli": "~3.6.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-typescript": "^1.3.1",
    "ember-cli-uglify": "^2.0.2",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "0.6.2",
    "ember-qunit": "^4.2.0",
    "ember-resolver": "^5.0.0",
    "ember-source": "~3.6.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-ember-suave": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "fs.extra": "^1.3.2",
    "husky": "^1.0.1",
    "lint-staged": "^8.0.4",
    "loader.js": "^4.2.3",
    "prettier": "^1.10.2",
    "typescript": "^3.0.1"
  },
  "resolutions": {
    "@types/ember__object": "~3.0.6",
    "@types/ember__component": "~3.0.4",
    "@types/ember__routing": "~3.0.6",
    "@types/ember__error": "~3.0.2",
    "@types/jquery": "~3.3.22"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/rwjblue/ember-lifeline/issues"
  },
  "homepage": "https://github.com/rwjblue/ember-lifeline#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
